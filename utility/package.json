{
    "name": "vscdevcon",
    "displayName": "VS Code Dev Containers",
    "description": "A utility to add VS Code Dev Container definitions to a local folder.",
    "repository": "https://github.com/Microsoft/vscode-dev-containers",
    "version": "0.0.1",
    "scripts": {
        "compile": "./node_modules/.bin/tsc -p ./",
        "watch": "./node_modules/.bin/tsc -watch -p ./",
        "prepack": "yarn run compile"
    },
    "main": "./out/devcon.js",
    "types": "./vscdevcon.ts",
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/node": "^10.12.21",
        "@types/yargs": "^12.0.10",
        "tslint": "^5.12.1",
        "typescript": "^3.3.1"
    },
    "dependencies": {
        "glob": "^7.1.3",
        "yargs": "^13.2.2"
    }
}

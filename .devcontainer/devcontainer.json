    // "See https://aka.ms/vscode-remote/devcontainer.json for format details."
    "name": "Dev Container Editing",
    "dockerFile": "Dockerfile",
    "extensions": [
        "yzhang.markdown-all-in-one",
        "streetsidesoftware.code-spell-checker",
